". This is a cell array of strings.\nmaskStr = get_param(gcb,'MaskValues');\n%"
" The outflag is the 8th mask parameter.\n    block = 'mute period [ms]';\n   "
" system_block = [gcb '/' block];\n    srcblock= 'S-Function';\n    system_src"
"block= [gcb '/' srcblock];\n    pos=get_param(system_block,'position');\n    "
"blocktype=get_param(system_block,'blocktype');\n% Check the value selected \n"
"if strcmp(maskStr{8},'on'),\n    if ~strcmp(blocktype,'Outport')\n        del"
"ete_line(gcb, [srcblock '/1'], [block '/1']);\n        delete_block(system_bl"
"ock);\n        add_block('simulink/Sinks/Out1',system_block,'position',pos);"
"\n        add_line(gcb, [srcblock '/1'], [block '/1']);\n    end\nelse\n    i"
"f ~strcmp(blocktype,'Terminator')\n        delete_line(gcb, [srcblock '/1'], "
"[block '/1']);\n        delete_block(system_block);\n        add_block('simul"
"ink/Sinks/Terminator',system_block,'position',pos);\n        add_line(gcb, [s"
"rcblock '/1'], [block '/1']);\n    end\nend\n%-------------------------------"
"--------------------------------------------\n% Mode input\n% Get the mask pa"
"rameter values. This is a cell array of strings.\nmaskStr = get_param(gcb,'Ma"
"skValues');\n% The doublet / triplet flag is the 9th mask parameter.\n    blo"
"ck = 'mode';\n    system_block = [gcb '/' block];\n    sinkblock= 'S-Function"
"';\n    system_sinkblock= [gcb '/' sinkblock];\n    pos=get_param(system_bloc"
"k,'position');\n    blocktype=get_param(system_block,'blocktype');\n% Check t"
"he value selected \nif strcmp(maskStr{9},'on'),\n    if ~strcmp(blocktype,'In"
"port')\n        delete_line(gcb, [block '/1'], [sinkblock '/3']);\n        de"
"lete_block(system_block);\n        add_block('simulink/Sources/In1',system_bl"
"ock,'Port','3','position',pos);\n        add_line(gcb, [block '/1'], [sinkblo"
"ck '/3']);\n    end\nelse % disable mode input, setit to zeros\n    if ~strcm"
"p(blocktype,'Constant')\n        delete_line(gcb, [block '/1'], [sinkblock '/"
"3']);\n        delete_block(system_block);\n         add_block('simulink/Sour"
"ces/Constant',system_block,'position',pos);\n        if exist('channelstim')"
"\n            channelstim=[];\n        end\n        set_param(system_block,'v"
"alue','zeros(1,length(channelstim))');\n        add_line(gcb, [block '/1'], ["
"sinkblock '/3']);\n    end\nend\n\n%-----------------------------------------"
"----------------------------------\n% default minimal Mask Display string\nMD"
"str=['image(imread(''RehaStim-background.png''), [0.05 0.25 0.9 0.9]);'...\n "
"   'port_label(''input'', 1, ''current'');'...\n    'port_label(''input'', 2,"
" ''pulsewidth'');'...\n    'text(0.25,0.1,[''Serial Port: COM'' num2str(com_p"
"ort)],''horizontalAlignment'',''left'',''verticalAlignment'',''bottom'');'..."
"\n    'text(0.25,0,[''Channels: '' num2str(channelstim)],''horizontalAlignmen"
"t'',''left'',''verticalAlignment'',''bottom'');'];\n% get the number of input"
"s and outputs and add additional labels if needed\nnum_ports=get_param(gcb,'P"
"orts');\nnum_inputs=num_ports(1); num_outputs=num_ports(2);\nif num_inputs>=3"
"\n    MDstr=[MDstr 'port_label(''input'', 3, ''mode'');'];\nend\nif num_outpu"
"ts>=1\n    MDstr=[MDstr 'port_label(''output'', 1, ''mute time [ms]'');'];\ne"
"nd\nset_param(gcb,'MaskDisplay',MDstr)\n"
      MaskDisplay	      "image(imread('RehaStim-background.png'), [0.05 "
"0.25 0.9 0.9]);port_label('input', 1, 'current');port_label('input', 2, 'puls"
"ewidth');text(0.25,0.1,['Serial Port: COM' num2str(com_port)],'horizontalAlig"
"nment','left','verticalAlignment','bottom');text(0.25,0,['Channels: ' num2str"
"(channelstim)],'horizontalAlignment','left','verticalAlignment','bottom');por"
"t_label('input', 3, 'mode');port_label('output', 1, 'mute time [ms]');"
